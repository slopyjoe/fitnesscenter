return this.each(function() {
    			$(this).keyup(function(e) {
    				if (e.keyCode == 13) {
    					$.get("/report/" + $('#employee_id').val(), function(data) {

    					});
    					$('#user_report').modal('show')
    				}
    			})
    		});
    	};
    	
    	.directive('ngOnkeyup', function() {
    return {
        restrict: 'A',
        scope: {
          func: '&ngOnkeyup'
        },
        link: function( scope, elem, attrs ) {
          elem.bind('keyup', scope.func(event));
        }
      };
    });
    function UserReportCtrl($scope) {
    	$scope.user = {"name" : "Nexus S"};
    	$scope.enter = function() {
    		alert("enter");
    	};
    }
    $.get("/report/" + $('#employee_id').val(), function(data) {
			user = {"name" : "Name"};
			
			data.user.name = data.user.firstName;
			$scope.user = data.user;
			
		});
